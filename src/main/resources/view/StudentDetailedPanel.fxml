<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox spacing="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Selected Student">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>
    <ListView prefHeight="300" id="detailedPanel" fx:id="studentDetailedView" VBox.vgrow="ALWAYS"/>
    <Label text="Assigned Courses">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>
    <ListView prefHeight="400" fx:id="courseListView" VBox.vgrow="ALWAYS"/>
</VBox>
